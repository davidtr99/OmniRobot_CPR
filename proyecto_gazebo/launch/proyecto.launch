<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find proyecto_gazebo)/worlds/proyecto.world"/>
    <!-- more default parameters can be changed here -->
  </include>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find omni_description)/urdf/description.urdf -urdf -x 0 -y 0 -z 0 -model omniRobot" />

  <node name="spawn_camera" pkg="gazebo_ros" type="spawn_model" args="-file $(find camera_description)/urdf/camera.urdf -urdf -x 0 -y 0 -z 1 -model CPRcamera" />

  <include file="$(find omnirobot_control)/launch/omnirobot_control.launch"/>
</launch>
